             x:Class="PrismWithRealm.Bundles.Interventions.Views.ViewA"
             Title="{Binding Title}">

    <ListView ItemsSource="{Binding Interventions}">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding Libelle}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>